<h1>Mikes App!</h1>
<button onclick="get('customers/new')">CREATE CUSTOMER</button>
<ul id="customers">
    <% for(var key in customers) { %>
        <% if (customers.hasOwnProperty(key)) { %>
            <li><%= customers[key].contactInfo.firstName %> <%= customers[key].contactInfo.lastName %> 
                <button onclick="get('customers/<%= customers[key].id %>')">View</button></li>
            <% } %>
        <% } %>
</ul>